}
img{max-width: 100%;height: auto;}
input[type="text"],
input[type="email"],
input[type="password"],
input[type="button"],
input[type="submit"],
input[type="tel"],
button{
	-webkit-appearance: none;
}
input[type="text"],
input[type="email"],
input[type="password"],
input[type="tel"]{
	height: 26px;
	padding:0 10px;
	font-size: 12px;
	font-family: Arial, sans-serif;
	border:1px solid #e6e6e6;	
}
input[type="submit"],
button{
	height: 26px;
	padding:0 10px;
	font-size: 12px;
	font-family: Arial, sans-serif;
	cursor: pointer;
}
textarea{
	padding:10px;
	font-size: 12px;
	font-family: Arial, sans-serif;
	border:1px solid #e6e6e6;	
}
table{
	 border-collapse: collapse;
	 border-spacing: 0;
}
iframe{
	max-width: 100%;
	border:none;
}
body{
	font-family: Arial,sans-serif;
