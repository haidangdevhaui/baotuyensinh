.nav-menu>ul{
	display: table;
	width: 100%;
	text-align: center;
	background: #0f90a4;	
	position: relative;
	z-index: 300;
}
.nav-menu>ul>li{
	display: table-cell;
	border-bottom: 2px solid #f58634;
	vertical-align: bottom;
}
.nav-menu>ul>li.current-menu {
	background: #f58634;
}
.nav-menu>ul>li.home>a{
	display: inline-block;
	width: 36px;
	height: 36px;
	background: url("../images/home-icon.png") no-repeat center;
}
.nav-menu>ul>li.home>a span{display: none;}
.nav-menu>ul>li.home {
	width: 36px;
	line-height: 0;
}
.nav-menu>ul>li>a {
	font-size: 14px;
	text-transform: uppercase;
	color: #fff;
	text-decoration: none;
	display: block;
	text-align: center;
	line-height: 1.14em;
	padding: 10px 0;
	position: relative;
}
.nav-menu>ul>li:hover>a{
	background-color: #F58634;
}
.nav-menu>ul>li>a:BEFORE,
.nav-menu>ul>li>a:after{
	width: 1px;
	height: 100%;	
	content: '';
	display: inline-block;
	background: url("../images/nav-sep.png") center right no-repeat;
	position: absolute;
	top: 0;	
}
.nav-menu>ul>li>a:after{
	background-position: left center;
	right: 0;
}
.nav-menu>ul>li>a:before{
	background-position: right center;
	left: 0;
}
.nav-menu>ul>li.current-menu>a:BEFORE,
.nav-menu>ul>li.current-menu>a:after,
.nav-menu>ul>li:FIRST-CHILD>a:before,
.nav-menu>ul>li:hover>a:before,
.nav-menu>ul>li:hover>a:after {
	display: none;
}
.nav-menu .sub-menu{
	position: absolute;
	width: 770px;
	background: rgba(15,144,164,0.9);
	text-align: left;
	top: 100%;
	padding: 5px;
	display: none;
}
.nav-menu .sub-right .sub-menu{
	right: 0;
}
.nav-menu li:hover .sub-menu{
	display: block;
}
.sub-menu .menu-title {
	font-size: 14px;
	text-transform: uppercase;
	font-weight: bold;	
	color: #fff;
	padding: 0 0 8px 10px;
	border-bottom: 1px solid #208292;
	box-shadow: 0 1px 0 #2fb4c8;
	line-height: 1em;
}
.sub-menu .list-smenu{
	width: 30%;
	float: left;
	padding: 20px;
}
.sub-menu .list-smenu a{
	display: block;
	color: #fff;
	font-size: 12px;
	font-weight: bold;
	line-height: 1.5em;
	padding: 7px 25px;
	text-decoration: none;	
	background: url("../images/dot-nav.png") no-repeat 10px center;
}
.sub-menu .list-smenu a:hover{
	opacity: 0.7;
}
.sub-menu .list-smenu ul li{
	border-top: 1px dashed #c0f7ff;
}
.sub-menu .list-smenu ul li:FIRST-CHILD {
	border-top: none; 
}
.sub-menu .nav-news,
.sub-menu .nav-feature {
	float: left;
	width: 35%;
	padding: 20px;
}
